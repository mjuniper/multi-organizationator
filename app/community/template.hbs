<h2>Showing the first 10 private items in the Community Org</h2>
<h3>Accessing community org as {{communityOrgService.portalInfo.user.fullName}}</h3>
<div class="row">
  <div class="col-md-12">
    {{#if model.results.length}}
      <table class="table table-responsive table-bordered table-hover table-striped">
        <thead>
          <tr>
            <th>Title</th>
            <th>Type</th>
            <th>Owner</th>
          </tr>
        </thead>
        <tbody>
          {{#each model.results as |item|}}
            <tr>
              <td>{{item.title}}</td>
              <td>{{item.type}}</td>
              <td>{{item.owner}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{/if}}
    {{#if error}}
      <div class="alert alert-danger" role="alert">{{error.message}}</div>
    {{/if}}
  </div>
</div>
